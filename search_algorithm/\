#include<stdio.h>

int BinarySearch(int *a, int len, int key){
	int low = 0;
	int high = len-1;
	while (low < high){
		int mid = (high - low) / 2;
		if (key > a[mid])
			low = mid + 1;
		else if (key < a[mid])
			high = mid - 1;
		else
			return mid;
	}
	
}

int main(){

}


